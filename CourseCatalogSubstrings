import requests
import re

ccURL = "https://raw.githubusercontent.com/annanardelli/SRP2021/main/CurriculumChart/b-s-in-computer-science-3.txt"
ccPage = requests.get(ccURL)
ccData = ccPage.text

substring = "CS-104"
position = re.finditer(substring, ccData)
matches_positions = [match.start() for match in position]
 
#position1 = re.finditer("Credits:", ccData)

print(matches_positions)
